{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "anhskohbo/no-captcha": "^2.3",
        "anlutro/l4-settings": "^0.5.0",
        "barryvdh/laravel-debugbar": "^2.4",
        "barryvdh/laravel-dompdf": "^0.8.1",
        "barryvdh/laravel-ide-helper": "^2.4",
        "caffeinated/modules": "^4.2",
        "doctrine/dbal": "^2.6",
        "dust-php/dust-php": "^0.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "5.4.*",
        "laravel/tinker": "~1.0",
        "laravelcollective/html": "^5.4.0",
        "milon/barcode": "^5.3",
        "nategood/httpful": "^0.2.20",
        "pragmarx/countries": "^0.1.1",
        "radic/blade-extensions": "^7.0",
        "rap2hpoutre/laravel-log-viewer": "^0.10.4",
        "spatie/laravel-permission": "^2.4",
        "vinkla/hashids": "^3.2",
        "webpatser/laravel-uuid": "2.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "laravel/homestead": "^6.1",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        },
        "files": [
            "app/Libs/blade.php",
            "app/Libs/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
